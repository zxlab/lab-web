@article{kongMicrobialNonvolatileChemical2022,
  title = {Microbial and {{Nonvolatile Chemical Diversities}} of {{Chinese Dark Teas Are Differed}} by {{Latitude}} and {{Pile Fermentation}}},
  author = {Kong, Ya-Shuai and Ren, Hong-Yu and Liu, Rui and {da Silva}, Ricardo R. and Aksenov, Alexander A. and Melnik, Alexey V. and Zhao, Ming and Le, Miao-Miao and Ren, Zhi-Wei and Xu, Feng-Qing and Yan, Xiao-Wei and Yu, Li-Jun and Zhou, Yu and Xie, Zhong-Wen and Li, Da-Xiang and Wan, Xiao-Chun and Long, Yan-Hua and Xu, Zhenjiang Zech and Ling, Tie-Jun},
